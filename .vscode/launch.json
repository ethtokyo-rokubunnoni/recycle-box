{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Next.js",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "dev"],
        "timeout": 60000,
        "console": "integratedTerminal",
        "autoAttachChildProcesses": true,
        "resolveSourceMapLocations": [
          "${workspaceFolder}/**",
          "!**/node_modules/**"
        ]
      }
    ]
  }
  